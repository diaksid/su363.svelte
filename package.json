{
  "name": "su363.svelte",
  "version": "1.1.4",
  "description": "construction company «CD 363»",
  "repository": {
    "type": "git",
    "url": "https://github.com/gkcck/su363.svelte.git"
  },
  "author": "DVA",
  "homepage": "https://su363.ru",
  "private": true,
  "scripts": {
    "clean": "rimraf .svelte-kit && rimraf build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "start": "node build/index.js",
    "cluster": "node run/cluster.js",
    "dev": "vite dev --port=3000",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "create:sitemap": "node run/create.sitemap.js",
    "create:iconset": "node run/create.iconset.js",
    "create:images": "node run/create.images.js",
    "generate:css": "npx tailwindcss -i ./static/assets/stylesheets/search.css -o ./static/assets/stylesheets/search.min.css --minify --watch",
    "build": "vite build",
    "preview": "vite preview --port=3000",
    "test": "playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@playwright/test": "*",
    "@sveltejs/adapter-node": "*",
    "@sveltejs/adapter-static": "*",
    "@sveltejs/kit": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "autoprefixer": "*",
    "cssnano": "*",
    "cssnano-preset-advanced": "*",
    "daks-svelte": "*",
    "dotenv": "*",
    "eslint": "*",
    "eslint-config-prettier": "*",
    "eslint-plugin-svelte3": "*",
    "postcss": "*",
    "postcss-import": "*",
    "postcss-load-config": "*",
    "prettier": "*",
    "prettier-plugin-svelte": "*",
    "rimraf": "*",
    "svelte": "*",
    "svelte-check": "*",
    "tailwindcss": "*",
    "tslib": "*",
    "typescript": "*",
    "vanilla-lazyload": "*",
    "vite": "*",
    "vite-imagetools": "*",
    "vitest": "*"
  },
  "type": "module"
}
